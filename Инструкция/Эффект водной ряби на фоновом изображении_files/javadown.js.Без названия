function setviewcookie(c) {
    if (c == 1) {
        var date = new Date();
        date.setDate(date.getDate() + 60);
        document.cookie = "type=1; path=/; expires=" + date.toUTCString();
		} else {
        var date = new Date(0);
        document.cookie = "type=; path=/; expires=" + date.toUTCString();
	}
    window.location.reload();
}
