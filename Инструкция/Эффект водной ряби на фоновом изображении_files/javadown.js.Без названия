function ValiDate() {
    var name = $("#name").val();
    var email = $("#email").val();
    var subject = $("#subject").val();
    var textarea = $("#textarea").val();
    var errormessage = "";
    if (name == "") {
        errormessage = errormessage + "<p>Вы не написали ваше имя.</p>";
        $("#name").addClass("er");
        $(".error").fadeIn(500);
		} else {
        $("#name").removeClass("er");
	}
    if (email == "") {
        errormessage = errormessage + "<p>Вы не написали ваш Email.</p>";
        $("#email").addClass("er");
        $(".error").fadeIn(500);
		} else if (!IsEmail(email)) {
        errormessage = errormessage + "<p>Вы написали не верный Email.</p>";
        $("#email").addClass("er");
        $(".error").fadeIn(500);
		} else {
        $("#email").removeClass("er");
	}
    if (textarea == "") {
        errormessage = errormessage + "<p>Вы не написали сообщение.</p>";
        $("#textarea").addClass("er");
        $(".error").fadeIn(500);
		} else {
        $("#textarea").removeClass("er");
	}
    if (errormessage == "") {
        var dataString = "name=" + name + "&email=" + email + "&subject=" + subject + "&text=" + textarea;
        $.ajax({
            type: "POST",
            url: "/contact.php",
            data: dataString,
            success: function (msg) {
                $("#mailform").fadeOut(500);
                $(".mailSuccessDiv").fadeIn(1500);
			},
		});
		} else {
        $(".error").html(errormessage);
	}
}
function fValiDate() {
    var name = $("#fname").val();
    var email = $("#femail").val();
    var subject = $("#fsubject").val();
    var textarea = $("#ftextarea").val();
    var errormessage = "";
    if (name == "") {
        errormessage = errormessage + "<p>Вы не написали ваше имя.</p>";
        $("#fname").addClass("fer");
        $(".ferror").fadeIn(500);
		} else {
        $("#fname").removeClass("fer");
	}
    if (email == "") {
        errormessage = errormessage + "<p>Вы не написали ваши контакты.</p>";
        $("#femail").addClass("fer");
        $(".ferror").fadeIn(500);
		} else {
        $("#femail").removeClass("fer");
	}
    if (textarea == "") {
        errormessage = errormessage + "<p>Вы не написали сообщение.</p>";
        $("#ftextarea").addClass("fer");
        $(".ferror").fadeIn(500);
		} else {
        $("#ftextarea").removeClass("fer");
	}
    if (errormessage == "") {
        var dataString = "name=" + name + "&email=" + email + "&subject=" + subject + "&text=" + textarea;
        $.ajax({
            type: "POST",
            url: "/contact.php",
            data: dataString,
            success: function (msg) {
                $("#fmailform").fadeOut(500);
                $(".fmailSuccessDiv").fadeIn(1500);
			},
		});
		} else {
        $(".ferror").html(errormessage);
	}
}

$(window).on('load', function () {     
	window.setTimeout(function() {
		$('.pace .pace-progress').addClass('hidden');
		$('.pace .pace-activity').addClass('hidden');
	}, 2000);
	//$('.sidebar-block .fixsidebar').css('opacity', '1');
});

$(document).ready(function () {
    "use strict";
    var isMobile;
    isMobile = navigator.userAgent.match(/(iPhone|iPod|Android|BlackBerry|iPad|IEMobile|Opera Mini)/);
    if (!isMobile) {
        $(window).scroll(function () {
            if ($(this).scrollTop() > 500) {
                $(".dmtop").css({ bottom: "8px" });
				} else {
                $(".dmtop").css({ bottom: "-80px" });
			}
            if ($(this).scrollTop() > 120) {
                $("#navigation").css({ top: "-80px" });
				$(document).mouseleave(function(e){
					if (e.clientY < 10) {
						$("#navigation").css({ top: "0" }); 
					}    
				});					
				} else {
                $("#navigation").css({ top: "0" });
			}			
		});
		
		if ($(window).scrollTop() > 120) {
			window.setTimeout(function() {
			$("#navigation").css({ top: "-80px" });
			}, 900);
		}
        $(".dmtop").on("click", function () {
            $("html, body").animate({ scrollTop: 0 }, 500);
            return !1;
		});
	}
    $(".pscroll").click(function () {
        var scroll_el = $(this).attr("href");
        if ($(scroll_el).length != 0) {
            $("html, body").animate({ scrollTop: $(scroll_el).offset().top }, 500);
		}
        return !1;
	});
    var iconOpen = "fa-minus",
	iconClose = "fa-plus";
    $(document).on("show.bs.collapse hide.bs.collapse", ".accordion", function (e) {
        var $target = $(e.target);
        $target
		.siblings(".accordion-heading")
		.find("i")
		.toggleClass(iconOpen + " " + iconClose);
        if (e.type == "show") $target.prev(".accordion-heading").find(".accordion-toggle").addClass("active");
        if (e.type == "hide") $(this).find(".accordion-toggle").not($target).removeClass("active");
	});
    $("[data-toggle=tooltip]").tooltip();
    $("[data-toggle=popover]").popover();
    $(".oneheight").matchHeight({ byRow: !1 });
    $(".onerowheight").matchHeight({ byRow: !0 });
    $(".fancybox").fancybox({
        padding: 0,
        openSpeed: 0,
        closeSpeed: 0,
        closeClick: !1,
        beforeClose: function () {
            $("#name2").removeClass("er");
            $("#email2").removeClass("er");
            $("#phone2").removeClass("er");
            $(".error2").hide();
            $(".mailSuccessDiv2").fadeOut();
            $("#mailform2").fadeIn();
            $("#sitename2").removeClass("er");
            $("#siteemail2").removeClass("er");
            $(".siteerror2").hide();
            $(".sitemailSuccessDiv2").fadeOut();
            $("#sitemailform2").fadeIn();
            $("#sitetextarea2").removeClass("er");
            $("body").removeClass("modalopen");
		},
        afterClose: function () {
            $(".dmtop").removeClass("hidden");
            $(".pace").removeClass("hidden");
            $(".fancybox-overlay").removeClass("fancybox-overlay-op");
            $(".fancybox-inner").removeClass("fancybox-inner-op");
		},
        helpers: { overlay: { locked: !1, closeClick: !1 } },
        beforeShow: function () {
            var alt = this.element.attr("data-title");
            var url = this.element.attr("data-url");
            if (!!alt && !!url) {
                this.title = '<div class="textbox"><h2><a href="/' + url + '/">' + alt + '</a></h2><p>' + this.title + '</p></div><div class="fanbot"><a href="/' + url + '/">Подробнее</a></div>';
				} else if (!!alt) {
                this.title = '<div class="textbox"><h2>' + alt + '</h2></div><div class="fanbot2">'+ this.title +'</div>';
				$(".fancybox-overlay").addClass("fancybox-overlay-op");
                $(".fancybox-inner").addClass("fancybox-inner-op");				
				} else {
                $(".fancybox-overlay").addClass("fancybox-overlay-op");
                $(".fancybox-inner").addClass("fancybox-inner-op");
			}
            $(".dmtop").addClass("hidden");
            $(".pace").addClass("hidden");
		},
	});
    $(".fancybox").click(function () {
        $("body").addClass("modalopen");
	});
    $("img.laz").Laz({
        threshold: 100
	});
	$(".sidebar").theiaStickySidebar({ additionalMarginTop: 30, additionalMarginBottom: 0 });
   	
});
function SiteValiDate2() {
    var name = $("#sitename2").val();
    var email = $("#siteemail2").val();
    var tnam = $("#sitetnam").val();
    var artikul = $("#siteartikul").val();
    var textarea = $("#sitetextarea2").val();
    var errormessage = "";
    if (name == "") {
        errormessage = errormessage + "<p>Вы не написали ваше имя.</p>";
        $("#sitename2").addClass("er");
        $(".siteerror2").fadeIn(500);
		} else {
        $("#sitename2").removeClass("er");
	}
    if (email == "") {
        errormessage = errormessage + "<p>Вы не написали ваш email.</p>";
        $("#siteemail2").addClass("er");
        $(".siteerror2").fadeIn(500);
		} else if (!IsEmail(email)) {
        errormessage = errormessage + "<p>Вы написали не верный email.</p>";
        $("#siteemail2").addClass("er");
        $(".siteerror2").fadeIn(500);
		} else {
        $("#siteemail2").removeClass("er");
	}
    if (textarea == "") {
        errormessage = errormessage + "<p>Вы не написали сообщение.</p>";
        $("#sitetextarea2").addClass("er");
        $(".siteerror2").fadeIn(500);
		} else {
        $("#sitetextarea2").removeClass("er");
	}
    if (errormessage == "") {
        var dataString = "name=" + name + "&email=" + email + "&text=" + textarea + "&tnam=" + tnam + "&artikul=" + artikul;
        $.ajax({
            type: "POST",
            url: "/sitecontact.php",
            data: dataString,
            success: function (msg) {
                $("#sitemailform2").fadeOut(500);
                $(".sitemailSuccessDiv2").fadeIn(1500);
                $("#sitetextarea2").val("");
			},
		});
		} else {
        $(".siteerror2").html(errormessage);
	}
}
